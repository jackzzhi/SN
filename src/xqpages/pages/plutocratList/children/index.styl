@import '../../../../assets/stylus/methods'
.bottom_msg_fixeds{
  width:100%
}
.content-plutocratList-price
  padding:0 15px;
  background:#fff;
  iphonefixpadding(102px)
  .plutocratList-price
    padding:0 15px;
    .title
      font-size: 16px;
      color: #010E4F;
      border-bottom: 1px solid #F1F1F1;
      padding:28px 0 10px 0;
    .plutocratList-price-hd
      display:flex;
      justify-content:space-between;
      padding:0 0 20px 0;
      .item
        padding-top:15px;
        flex:1;
        .one
          width:100%;
          height:52px;
          background:url(../../../assets/images/no1.png);
          background-size:44px 52px;
          background-repeat:no-repeat;
          background-position:bottom center;
        .two
          width:100%;
          height:52px;
          background:url(../../../assets/images/no2.png);
          background-size:44px 52px;
          background-repeat:no-repeat;
          background-position:center bottom;
        .three
          width:100%;
          height:52px;
          background:url(../../../assets/images/no3.png);
          background-size:44px 52px;
          background-repeat:no-repeat;
          background-position:bottom center;
        .person
          padding-top:6px;
          font-size: 12px;
          color: #A6A6A6;
          text-align:center
        .price
          font-size: 18px;
          color:#010E4F;
          line-height:35px;
          text-align:center
        .time
          font-size: 10px;
          color:#6C7B8A;
          line-height:15px;
          text-align:center
.table
  width:100%;
  background:#fff;
  .th-block
    display:flex;
    .th
      height:45px;
      line-height:45px;
      flex:1;
      font-size: 14px;
      color: #010E4F;
      text-align:center;
      font-weight:700;
      overflow:hidden;
  .content-block
    &:nth-of-type(odd)
      background: #fff;
    &:nth-of-type(even)
      background: #FBFAFA;
    .content-block-hd
      display:flex;
      .td2
        line-height 50px
      .td
        flex:1;
        font-size: 14px;
        color: #29354D;
        text-align:center;
        overflow:hidden;
        padding:17px 0;
        .time
          font-size: 10.8px;
          color: #6C7B8A;
          margin-top:10px;
          margin-left:-9px;
        .price
          font-size: 16px;
          color: #010E4F;
          .wan
            font-size: 9.6px;
            color: #010000;
        &.normal
          font-size: 20px;
          color: #29354D;
          line-height 36px
        &.one
          width:19px;
          background:url(../../../assets/images/a.png)
          background-repeat:no-repeat;
          background-size:19px 25px;
          background-position:center center;
        &.two
          width:19px;
          background:url(../../../assets/images/b.png)
          background-repeat:no-repeat;
          background-size:19px 25px;
          background-position:50% 40%;
        &.three
          width:19px;
          background:url(../../../assets/images/c.png)
          background-repeat:no-repeat;
          background-size:19px 25px;
          background-position:center center;
        &.shouqi
          width:19px;
          background:url(../../../assets/images/shouqi.png)
          background-repeat:no-repeat;
          background-size:9px 6px;
          background-position:center center;
        &.zhankai
          width:19px;
          background:url(../../../assets/images/zhankai.png)
          background-repeat:no-repeat;
          background-size:9px 6px;
          background-position:center center;
    .content-block-detail
      padding:0 55px;
      background:#fff;
      .content-block-detail-tr
        display:flex;
        height:56px;
        line-height:56px;
        position:relative;
        .time
          font-size: 10.8px;
          color: #6C7B8A;

        .item
          flex:1;
          font-size: 14px;
          color: #29354D;

          &.price
            font-size: 16px;
            color: #010E4F;
            text-align: center;
            .wan
              font-size: 9.6px;
              color: #010000;
.hr
  width:100%;
  height:10px;
  background: #F7F7F7;
  
.mint-loadmore-bottom
  height 40px
  line-height 40px
  display: flex
  align-items: center
  text-align: center
  justify-content: center
  .icon
    display inline-block
    margin 0 10px
.no-data
  height 54px
  line-height 54px
  font-size 14px
  color #909090
  display flex
  align-items center
  justify-content center
  span
    display inline-block
    width 68px
    height 3px
    margin 0 20px;
    background-size 68px 3px;
    background-repeat no-repeat
    &.line1
      background-image url("../../../assets/images/line1.png")
    &.line2
      background-image url("../../../assets/images/line2.png")
.bottom_msg_fixed
  position:fixed
  bottom: 0px
  width:100%
.bottom_msg
  padding:17px 0 17px 0;
  background: #FCFDFF;
  text-align:center
  width:100%;
  span
    color:#8D92A9
    font-size:12px
  .dots
    width:4px
    height:4px
    background:#8d92a9
    border-radius:50%
    display: inline-block
    margin-bottom: 2px
.line
  width:108%;
  height:1px;
  background:#f2f2f2;
  position:absolute;
  bottom:0;
  margin-left:-4%;
