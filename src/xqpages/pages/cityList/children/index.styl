@import '../../../../assets/stylus/methods'
.first{
  width:130px;
}

.second{
  width:70px;
}
.bottom_msg_fixeds{
  width:100%
}
.wrapper-citylist-price
  padding-left: 15px;
  padding-right: 15px;
  background:#fff;
  iphonefixpadding(100px)
  .table
    width:100%;
    background:#fff;
    tr
      border-bottom:1px solid #F9F9F9;
      td
        font-size: 14px;
        color: #010E4F;
        padding:13px 0;
        text-align:center;
        position :relative;
      th
        font-weight:700;
        padding:16px 0;
        font-size: 14px;
        color: #010E4F;
        text-align:center;


        &:last-child
          width:130px;
  .bottom-fixed
   position:fixed;
   bottom:0;
   left:0;
   width:100%;
   height:40px;
   line-height:40px;
   font-size: 12px;
   color: #8D92A9;
   text-align:center
   background:#fff;
  .topBar
    position fixed
    iphonefixbottom(60px) 
    right 20px
    img
     width 40px
     height 40px
.mint-loadmore-bottom
  height 40px
  line-height 40px
  display: flex
  align-items: center
  text-align: center
  justify-content: center
  .icon
    display inline-block
    margin 0 10px
.no-data
  height 54px
  line-height 54px
  font-size 14px
  color #909090
  display flex
  align-items center
  justify-content center
  span
    display inline-block
    width 68px
    height 3px
    margin 0 20px;
    background-size 68px 3px;
    background-repeat no-repeat
    &.line1
      background-image url("../../../assets/images/line1.png")
    &.line2
      background-image url("../../../assets/images/line2.png")

.bottom_msg_fixed
  position:fixed
  width:100%
  bottom:0px
.bottom_msg
  padding:17px 0 17px 0;
  background: #FCFDFF;
  text-align:center
  width:100%;
  span
    color:#8D92A9
    font-size:12px
  .dots
    width:4px
    height:4px
    background:#8d92a9
    border-radius:50%
    display: inline-block
    margin-bottom: 2px
