@import "../../../assets/stylus/methods.styl"
.fixed-container
  position absolute
  left 0px
  right 0px
  background-color #f6f8f9
  padding-bottom 96px;
  font-family PingFangSC-Regular
  font-size 14px
  color #29354D
  img
    width 26px
    height 26px
    margin-right 2px
.loadmores
  iphonefixpaddingb(0px)

.no-data
  height 54px
  line-height 54px
  font-size 14px
  color #909090
  display flex
  align-items center
  justify-content center
  span
    display inline-block
    width 68px
    height 3px
    margin 0 20px;
    background-size 68px 3px;
    background-repeat no-repeat
  span.line1
    background-image url("../../../assets/images/line1.png")
  span.line2
    background-image url("../../../assets/images/line2.png")
.mint-loadmore-top
  padding-top 10px
  height 40px
  line-height 40px
  display: flex;
  align-items: center;
  text-align: center;
  justify-content: center;
  .icon
    display inline-block
    margin 0 10px
.mint-loadmore-bottom
  height 40px
  line-height 40px
  display: flex;
  align-items: center;
  text-align: center;
  justify-content: center;
  .icon
    display inline-block
    margin 0 10px
.msite_wrap
  background-color #F6F8F9
.msite_head
  width 100%
  height 58px
  text-align center
  background-color white
  border-bottom 0.5px solid #f1f1f1
  .city_all
    font-size 16px
    line-height 70px
  .city_open
    width 12px
    height 13px

.topBar
  position fixed
  iphonefixbottom(60px)
  right 20px
  img
    width 40px
    height 40px
.tab-container
  iphonefixpadding(115px)

